
#~define CROP
#~define CUT
#~define CLIP


Main:
~if CROP
	set TestCircleCrop
~elseif CUT
	set TestCircleCut
~elseif CLIP
	set TestCircleClip
~else
	set DrawCircle
~endif


TestCircleCrop:
	crop DrawCircle 0.1 0.1 0 0.5 0.5 1

TestCircleCut:
	cut_box DrawCircle 0.1 0.1 0 0.5 0.5 1

TestCircleClip:
	clip_box DrawCircle 0.1 0.1 0 0.5 0.5 1


DrawCircle:
	setactive 1 2 3
	circle1 0.5 0.5 0.5