  </PropertyGroup>
  <ItemGroup>
    <None Remove="assets\filter\blue.fl" />
    <None Remove="assets\filter\red.fl" />
    <None Remove="assets\test_kernel\red.cl" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="assets\filter\blue.fl" />
    <EmbeddedResource Include="assets\filter\red.fl" />
