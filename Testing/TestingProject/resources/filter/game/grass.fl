--define texture temp: empty

Perlin: #Creates perlin noise
  setactive 0 1 2
  urnd
  perlin 0.9 4

Main:
  dSet_v 1
  setactive 0 1 2
  dSet_t Perlin
  dClampRescale_vv 0.5 0.6

  setactive temp 0 1 2
  dSet_t in
  dFlip
  setactive 0 2
  dSet_v 0
  setactive 0 1 2
  dMul_v 0.1

  setactive in 0 2
  dSet_v 0
  setactive 0 1 2 
  setactive 0 1 2
  dMul_v 0.3
  dAdd_t temp