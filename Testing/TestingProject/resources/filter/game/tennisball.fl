
CreateMask: static
  setactive 3
  setv 1
  setactive 0 1 2
  circle1 0.5 0.5 0.4
  invert
  circle1 0.5 0.5 0.3

CreateMaskInv:
  jmp CreateMask
  invert

Perlin: static
  setactive 3
  setv 1
  setactive 0 1 2
  urnd
  perlin 0.8 7

YPerlin: static
  setactive 3
  setv 1
  setactive 1 2
  urnd
  perlin 0.8 7
  

YellowPerlin:
  jmp YPerlin
  adjustlevelrescale 0.4 0.6
  adjustlevel 0.9 1


White: static
  setactive 0 1 2
  setv 1

Main:  
  setactive 3
  setv 1
  setactive 1 2
  mixt YellowPerlin CreateMask
  mulv 2
  setactive 0 1 2
  mixt White CreateMaskInv
 