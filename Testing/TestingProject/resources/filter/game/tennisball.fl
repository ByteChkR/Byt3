
CreateMask:
  setactive 3
  setv 1
  setactive 0 1 2
  circle1 0.5 0.5 0.4
  invert
  circle1 0.5 0.5 0.3

CreateMaskInv:
  jmp CreateMask
  invert

Perlin:
  setactive 3
  setv 1
  setactive 0 1 2
  urnd
  perlin 0.8 7

YPerlin:
  setactive 3
  setv 1
  setactive 1 2
  urnd
  perlin 0.8 7
  

YellowPerlin:
  jmp YPerlin
  dAdjustLevelRescale_c_vv 0.4 0.6
  #adjustlevelrescale 0.4 0.6
  adjustlevel 0.9 1


White:
  setactive 0 1 2
  setv 1

Main:  
  setactive 3
  setv 1
  setactive 1 2
  mixt YellowPerlin CreateMask
  mulv 2
  setactive 0 1 2
  mixt White CreateMaskInv
 